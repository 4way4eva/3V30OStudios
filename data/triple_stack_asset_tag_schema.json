  "neural_loop_connections": {
    "description": "Automatic neural loop activation based on asset tag patterns",
    "rules": [
      {
        "condition": "asset_tag matches /^CIV::ES0IL::.*/",
        "activates": "LOOP::CIVILIAN::INFRASTRUCTURE"
      },
      {
        "condition": "asset_tag matches /^CIV::EVOL::WEAR::.*/",
        "activates": "LOOP::CIVILIAN::IDENTITY"
      },
      {
        "condition": "asset_tag matches /^CIV::EDU::.*/",
        "activates": "LOOP::CIVILIAN::KNOWLEDGE"
      },
      {
        "condition": "asset_tag matches /^CIV::(FOOD|WATER|ENERGY)::.*/",
        "activates": "LOOP::CIVILIAN::COMMERCE"
      },
      {
        "condition": "asset_tag matches /^MIL::DEF::.*/",
        "activates": "LOOP::MILITARY::PROTECTION"
      },
      {
        "condition": "asset_tag matches /^MIL::(ORB|MAR)::.*/",
        "activates": "LOOP::MILITARY::LOGISTICS"
      },
      {
        "condition": "asset_tag matches /^MIL::AI::.*/",
        "activates": "LOOP::MILITARY::INTELLIGENCE"
      },
      {
        "condition": "asset_tag matches /^COS::GATE::.*/",
        "activates": "LOOP::COSMIC::GATEWAY"
      },
      {
        "condition": "asset_tag matches /^COS::QM::.*/",
        "activates": "LOOP::COSMIC::SUBSTANCE"
      },
      {
        "condition": "asset_tag matches /^COS::(TRANS|LEDGER)::.*/",
        "activates": "LOOP::COSMIC::TRANSIT"
      }
    ]
  },
  
