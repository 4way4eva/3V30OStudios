# Deployer Private Key (REQUIRED - DO NOT COMMIT THIS FILE)
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Recipient address for minted ENFT (REQUIRED)
# This can be from PayString or your wallet address
RECIPIENT=0xYourWalletAddressHere

# IPFS CID from War Codex package upload (REQUIRED)
# Get this from: ipfs add -r EVOL_War_Codex_Mint_Package
CID=QmYourIPFSCIDHere

# Contract address (OPTIONAL - leave empty to deploy new)
# Use the existing contract if it has safeMint function
CONTRACT=0x638f2c25dc4346dbef5566a2d5da871f6d268b8a

# RPC URLs (default values work, but you can customize)
AVALANCHE_RPC_URL=https://api.avax.network/ext/bc/C/rpc
FUJI_RPC_URL=https://api.avax-test.network/ext/bc/C/rpc

# Snowtrace API Key (for contract verification - optional)
SNOWTRACE_API_KEY=your_snowtrace_api_key_here
